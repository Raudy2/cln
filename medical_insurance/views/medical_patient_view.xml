<?xml version="1.0"?>
<odoo>
	<data>
		<record id="medical_patient_view_form" model="ir.ui.view">
			<field name="name">medical.patient.form.inherit</field>
			<field name="model">medical.patient</field>
			<field name="inherit_id" ref="medical_base.medical_patient_form_view"/>
			<field name="arch" type="xml">
				<xpath expr="//notebook" position="inside">
					<page string="Insurance Providers">
						<group>
							<field name="insurance_provider_ids" />
						</group>
					</page>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
